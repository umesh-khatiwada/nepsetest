<%- include('../views/partials/header.ejs'); %>
  <%- include('../views/partials/navbar.ejs'); %>


    <main role="main" class="container">

      <div class="jumbotron text-center">


        <h1>Search</h1>

        <form action="/nepsearch" method="POST">
          <input id="nepseSearch" type="text" placeholder="Search.." name="npsearch">
          <button type="submit">Submit</button>
        </form>



        <%var searchdata;%>


          <%if (searchdata !=null ){ %>

            <% const mylength=searchdata.length %>




              <% for(var i=0;i<mylength;i++){ %>

                <p class="form-control mr-sm-2">
                  <%=searchdata[i].securityName%>
                </p>

                <% } %>

                  <% } else {%>
                    <p type="text" class="form-control mr-sm-2"> <%=searchdata%> no data found</p>

                    <% }%>



      </div>
    </main>





    <div class="form-group">

      <input type="text" class="form-control form-control-lg" id="searchh">



    </div>

    <div id="match-list"> </div>









